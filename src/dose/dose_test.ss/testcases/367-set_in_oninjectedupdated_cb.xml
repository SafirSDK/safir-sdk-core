<?xml version="1.0" encoding="utf-8"?>
<DoseTest.Items.TestCase>
  <Description xml:space="preserve">Test a SetAll call in an OnInjectedUpdated callback.</Description>
  <Expectation xml:space="preserve">partner 0/consumer 0 receives an OnInitialInjectionsDone, an OnInjectedNewEntity, and an OnInjectedUpdatedEntity. The Partner 2 read value 434343 for member Int64Member.</Expectation>
  <TestCaseSetupActions>
    <DoseTest.Action index="0">
      <ActionKind>RegisterEntityHandlerInjection</ActionKind>
      <Partner>0</Partner>
      <Consumer>0</Consumer>
      <TypeId>DoseTest.InjectableEntity</TypeId>
      <Handler>DEFAULT_HANDLER</Handler>
      <InstanceIdPolicy>RequestorDecidesInstanceId</InstanceIdPolicy>
    </DoseTest.Action>
    <DoseTest.Action index="1">
      <ActionKind>SetAll</ActionKind>
      <Partner>0</Partner>
      <ActionCallback>OnInjectedUpdatedEntity</ActionCallback>
      <Consumer>0</Consumer>
      <Instance>654321</Instance>
      <Handler>DEFAULT_HANDLER</Handler>
      <Object type="DoseTest.InjectableEntity">
        <Int64Member>434343</Int64Member>
      </Object>
    </DoseTest.Action>
  </TestCaseSetupActions>
  <TestActions>
    <DoseTest.Action index="0">
      <ActionKind>InjectChanges</ActionKind>
      <Partner>1</Partner>
      <Consumer>1</Consumer>
      <Instance>654321</Instance>
      <Handler>DEFAULT_HANDLER</Handler>
      <Object type="DoseTest.InjectableEntity">
        <Int64Member>99999999</Int64Member>
      </Object>
      <TimestampDelta>10</TimestampDelta>
    </DoseTest.Action>
    <DoseTest.Action index="1">
      <ActionKind>InjectChanges</ActionKind>
      <Partner>1</Partner>
      <Consumer>1</Consumer>
      <Instance>654321</Instance>
      <Handler>DEFAULT_HANDLER</Handler>
      <Object type="DoseTest.InjectableEntity">
        <Int64Member>777666</Int64Member>
      </Object>
      <TimestampDelta>10</TimestampDelta>
    </DoseTest.Action>
    <DoseTest.Action index="2">
      <ActionKind>Sleep</ActionKind>
      <SleepDuration>3</SleepDuration>
    </DoseTest.Action>
    <DoseTest.Action index="3">
      <ActionKind>Read</ActionKind>
      <Partner>2</Partner>
      <Consumer>2</Consumer>
      <EntityId>
        <name>DoseTest.InjectableEntity</name>
        <instanceId>654321</instanceId>
      </EntityId>
    </DoseTest.Action>
    <DoseTest.Action index="4">
      <ActionKind>DeleteAllInstances</ActionKind>
      <Partner>0</Partner>
      <Consumer>0</Consumer>
      <TypeId>DoseTest.InjectableEntity</TypeId>
      <Handler>DEFAULT_HANDLER</Handler>
    </DoseTest.Action>
  </TestActions>
</DoseTest.Items.TestCase>
