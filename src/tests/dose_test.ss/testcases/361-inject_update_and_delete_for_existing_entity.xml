<?xml version="1.0" encoding="utf-8"?>
<DoseTest.Items.TestCase>
  <Description xml:space="preserve">Inject update and inject delete for an existing entity</Description>
  <Expectation xml:space="preserve">partner 0/consumer 0 receives one OnInitialInjectionsDone, one OnInjectedNewEntity, one OnInjectedUpdatedEntity and one OnInjectedDeletedEntity. P1 and P2 reads the entity before and after each injection.</Expectation>
  <TestCaseSetupActions>
    <DoseTest.Action>
      <ActionKind>RegisterEntityHandlerInjection</ActionKind>
      <Partner>0</Partner>
      <Consumer>0</Consumer>
      <TypeId>DoseTest.InjectableEntity</TypeId>
      <Handler>DEFAULT_HANDLER</Handler>
      <InstanceIdPolicy>RequestorDecidesInstanceId</InstanceIdPolicy>
    </DoseTest.Action>
  </TestCaseSetupActions>
  <TestActions>
    <DoseTest.Action>
      <ActionKind>InjectChanges</ActionKind>
      <Partner>1</Partner>
      <Consumer>1</Consumer>
      <Instance>654321</Instance>
      <Handler>DEFAULT_HANDLER</Handler>
      <Object type="DoseTest.InjectableEntity">
        <Info xml:space="preserve">First inject</Info>
      </Object>
      <TimestampDelta>10</TimestampDelta>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>Sleep</ActionKind>
      <SleepDuration>1</SleepDuration>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>Read</ActionKind>
      <Partner>1</Partner>
      <Consumer>0</Consumer>
      <EntityId>
        <name>DoseTest.InjectableEntity</name>
        <instanceId>654321</instanceId>
      </EntityId>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>Read</ActionKind>
      <Partner>2</Partner>
      <Consumer>0</Consumer>
      <EntityId>
        <name>DoseTest.InjectableEntity</name>
        <instanceId>654321</instanceId>
      </EntityId>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>InjectChanges</ActionKind>
      <Partner>1</Partner>
      <Consumer>1</Consumer>
      <Instance>654321</Instance>
      <Handler>DEFAULT_HANDLER</Handler>
      <Object type="DoseTest.InjectableEntity">
        <Info xml:space="preserve">Second inject</Info>
      </Object>
      <TimestampDelta>20</TimestampDelta>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>Sleep</ActionKind>
      <SleepDuration>1</SleepDuration>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>Read</ActionKind>
      <Partner>1</Partner>
      <Consumer>0</Consumer>
      <EntityId>
        <name>DoseTest.InjectableEntity</name>
        <instanceId>654321</instanceId>
      </EntityId>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>Read</ActionKind>
      <Partner>2</Partner>
      <Consumer>0</Consumer>
      <EntityId>
        <name>DoseTest.InjectableEntity</name>
        <instanceId>654321</instanceId>
      </EntityId>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>InjectDelete</ActionKind>
      <Partner>1</Partner>
      <Consumer>1</Consumer>
      <Handler>DEFAULT_HANDLER</Handler>
      <EntityId>
        <name>DoseTest.InjectableEntity</name>
        <instanceId>654321</instanceId>
      </EntityId>
      <TimestampDelta>30</TimestampDelta>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>Sleep</ActionKind>
      <SleepDuration>1</SleepDuration>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>Read</ActionKind>
      <Partner>1</Partner>
      <Consumer>0</Consumer>
      <EntityId>
        <name>DoseTest.InjectableEntity</name>
        <instanceId>654321</instanceId>
      </EntityId>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>Read</ActionKind>
      <Partner>2</Partner>
      <Consumer>0</Consumer>
      <EntityId>
        <name>DoseTest.InjectableEntity</name>
        <instanceId>654321</instanceId>
      </EntityId>
    </DoseTest.Action>
  </TestActions>
</DoseTest.Items.TestCase>
