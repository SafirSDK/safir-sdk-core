<?xml version="1.0" encoding="utf-8"?>
<DoseTest.Items.TestCase>
  <Description xml:space="preserve">Test of registration subscription including subclasses (global)</Description>
  <Expectation xml:space="preserve">Partner 2/Consumer 2 receives OnRegistered for ROOT_HANDLER_0, GLOBAL_HANDLER_0 and GLOBAL_HANDLER_1</Expectation>
  <TestCaseSetupActions>
    <DoseTest.Action>
      <ActionKind>RegisterServiceHandler</ActionKind>
      <Partner>1</Partner>
      <Consumer>1</Consumer>
      <TypeId>DoseTest.RootService</TypeId>
      <Handler>ROOT_HANDLER_0</Handler>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>RegisterServiceHandler</ActionKind>
      <Partner>1</Partner>
      <Consumer>1</Consumer>
      <TypeId>DoseTest.GlobalService</TypeId>
      <Handler>GLOBAL_HANDLER_0</Handler>
    </DoseTest.Action>
  </TestCaseSetupActions>
  <TestActions>
    <DoseTest.Action>
      <ActionKind>SubscribeRegistration</ActionKind>
      <Partner>2</Partner>
      <Consumer>2</Consumer>
      <TypeId>DoseTest.RootService</TypeId>
      <Handler>ALL_HANDLERS</Handler>
      <IncludeSubclasses>true</IncludeSubclasses>
      <RestartSubscription>false</RestartSubscription>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>UnsubscribeRegistration</ActionKind>
      <Partner>2</Partner>
      <Consumer>2</Consumer>
      <TypeId>DoseTest.RootService</TypeId>
      <Handler>ALL_HANDLERS</Handler>
      <IncludeSubclasses>false</IncludeSubclasses>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>RegisterServiceHandler</ActionKind>
      <Partner>1</Partner>
      <Consumer>1</Consumer>
      <TypeId>DoseTest.RootService</TypeId>
      <Handler>ROOT_HANDLER_1</Handler>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>RegisterServiceHandler</ActionKind>
      <Partner>1</Partner>
      <Consumer>1</Consumer>
      <TypeId>DoseTest.GlobalService</TypeId>
      <Handler>GLOBAL_HANDLER_1</Handler>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>UnsubscribeRegistration</ActionKind>
      <Partner>2</Partner>
      <Consumer>2</Consumer>
      <TypeId>DoseTest.RootService</TypeId>
      <Handler>ALL_HANDLERS</Handler>
      <IncludeSubclasses>true</IncludeSubclasses>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>RegisterServiceHandler</ActionKind>
      <Partner>1</Partner>
      <Consumer>1</Consumer>
      <TypeId>DoseTest.GlobalService</TypeId>
      <Handler>GLOBAL_HANDLER_2</Handler>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>Sleep</ActionKind>
      <SleepDuration>5</SleepDuration>
    </DoseTest.Action>
  </TestActions>
</DoseTest.Items.TestCase>
