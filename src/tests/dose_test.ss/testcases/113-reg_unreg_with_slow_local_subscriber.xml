<?xml version="1.0" encoding="utf-8"?>
<DoseTest.Items.TestCase>
  <Description xml:space="preserve">Test of registering a service handler while there is an unregister state remaining (local)</Description>
  <Expectation xml:space="preserve">Consumer 0 receives one OnRegistered</Expectation>
  <TestCaseSetupActions>
    <DoseTest.Action>
      <ActionKind>SubscribeRegistration</ActionKind>
      <Partner>0</Partner>
      <Consumer>0</Consumer>
      <TypeId>DoseTest.LocalService</TypeId>
      <Handler>ALL_HANDLERS</Handler>
      <IncludeSubclasses>true</IncludeSubclasses>
      <RestartSubscription>false</RestartSubscription>
    </DoseTest.Action>
  </TestCaseSetupActions>
  <TestActions>
    <DoseTest.Action>
      <ActionKind>Sleep</ActionKind>
      <Partner>0</Partner>
      <SleepDuration>2</SleepDuration>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>RegisterServiceHandler</ActionKind>
      <Partner>1</Partner>
      <Consumer>0</Consumer>
      <TypeId>DoseTest.LocalService</TypeId>
      <Handler>DEFAULT_HANDLER</Handler>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>Close</ActionKind>
      <Partner>1</Partner>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>Open</ActionKind>
      <Partner>1</Partner>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>RegisterServiceHandler</ActionKind>
      <Partner>1</Partner>
      <Consumer>0</Consumer>
      <TypeId>DoseTest.LocalService</TypeId>
      <Handler>DEFAULT_HANDLER</Handler>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>Sleep</ActionKind>
      <SleepDuration>4</SleepDuration>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>UnsubscribeRegistration</ActionKind>
      <Partner>0</Partner>
      <Consumer>0</Consumer>
      <TypeId>DoseTest.LocalService</TypeId>
      <Handler>ALL_HANDLERS</Handler>
      <IncludeSubclasses>true</IncludeSubclasses>
    </DoseTest.Action>
  </TestActions>
</DoseTest.Items.TestCase>
