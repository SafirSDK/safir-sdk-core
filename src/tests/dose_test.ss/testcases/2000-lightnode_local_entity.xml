<?xml version="1.0" encoding="utf-8"?>
<DoseTest.Items.TestCase>
  <Description xml:space="preserve">Register a local entity on a lightnode (partner 4)</Description>
  <Expectation xml:space="preserve">No other node should receive registration, but of course local should</Expectation>
  <TestCaseSetupActions>
    <DoseTest.Action>
      <ActionKind>SubscribeRegistration</ActionKind>
      <Partner>0</Partner>
      <Consumer>0</Consumer>
      <TypeId>DoseTest.LocalEntity</TypeId>
      <Handler>ALL_HANDLERS</Handler>
      <IncludeSubclasses>true</IncludeSubclasses>
      <RestartSubscription>false</RestartSubscription>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>SubscribeRegistration</ActionKind>
      <Partner>2</Partner>
      <Consumer>0</Consumer>
      <TypeId>DoseTest.LocalEntity</TypeId>
      <Handler>ALL_HANDLERS</Handler>
      <IncludeSubclasses>true</IncludeSubclasses>
      <RestartSubscription>false</RestartSubscription>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>SubscribeRegistration</ActionKind>
      <Partner>3</Partner>
      <Consumer>0</Consumer>
      <TypeId>DoseTest.LocalEntity</TypeId>
      <Handler>ALL_HANDLERS</Handler>
      <IncludeSubclasses>true</IncludeSubclasses>
      <RestartSubscription>false</RestartSubscription>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>SubscribeRegistration</ActionKind>
      <Partner>4</Partner>
      <Consumer>0</Consumer>
      <TypeId>DoseTest.LocalEntity</TypeId>
      <Handler>ALL_HANDLERS</Handler>
      <IncludeSubclasses>true</IncludeSubclasses>
      <RestartSubscription>false</RestartSubscription>
    </DoseTest.Action>

    <DoseTest.Action>
      <ActionKind>SubscribeEntity</ActionKind>
      <Partner>0</Partner>
      <Consumer>0</Consumer>
      <TypeId>DoseTest.LocalEntity</TypeId>
      <IncludeUpdates>true</IncludeUpdates>
      <IncludeSubclasses>false</IncludeSubclasses>
      <RestartSubscription>true</RestartSubscription>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>SubscribeEntity</ActionKind>
      <Partner>2</Partner>
      <Consumer>0</Consumer>
      <TypeId>DoseTest.LocalEntity</TypeId>
      <IncludeUpdates>true</IncludeUpdates>
      <IncludeSubclasses>false</IncludeSubclasses>
      <RestartSubscription>true</RestartSubscription>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>SubscribeEntity</ActionKind>
      <Partner>3</Partner>
      <Consumer>0</Consumer>
      <TypeId>DoseTest.LocalEntity</TypeId>
      <IncludeUpdates>true</IncludeUpdates>
      <IncludeSubclasses>false</IncludeSubclasses>
      <RestartSubscription>true</RestartSubscription>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>SubscribeEntity</ActionKind>
      <Partner>4</Partner>
      <Consumer>0</Consumer>
      <TypeId>DoseTest.LocalEntity</TypeId>
      <IncludeUpdates>true</IncludeUpdates>
      <IncludeSubclasses>false</IncludeSubclasses>
      <RestartSubscription>true</RestartSubscription>
    </DoseTest.Action>
  </TestCaseSetupActions>
  <TestActions>
    <DoseTest.Action>
      <ActionKind>RegisterEntityHandler</ActionKind>
      <Partner>4</Partner>
      <Consumer>2</Consumer>
      <TypeId>DoseTest.LocalEntity</TypeId>
      <Handler>DEFAULT_HANDLER</Handler>
      <InstanceIdPolicy>RequestorDecidesInstanceId</InstanceIdPolicy>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>SetChanges</ActionKind>
      <Partner>4</Partner>
      <Consumer>2</Consumer>
      <Instance>0</Instance>
      <Handler>DEFAULT_HANDLER</Handler>
      <Object type="DoseTest.LocalEntity">
        <Info xml:space="preserve">Some Data</Info>
      </Object>
    </DoseTest.Action>
    <DoseTest.Action>
      <ActionKind>Delete</ActionKind>
      <Partner>4</Partner>
      <Consumer>2</Consumer>
      <Handler>DEFAULT_HANDLER</Handler>
      <EntityId>
        <name>DoseTest.LocalEntity</name>
        <instanceId>0</instanceId>
      </EntityId>
    </DoseTest.Action>
  </TestActions>
</DoseTest.Items.TestCase>
