<?xml version="1.0" encoding="utf-8" ?>
<class xmlns="urn:safir-dots-unit" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <summary>Contains the parametrization of queue lengths used by a connection.</summary>
    <name>Safir.Dob.QueueParameters</name>
    <baseClass>Safir.Dob.Parametrization</baseClass>
    <parameters>
        <parameter>
          <name>QueueRules</name>
          <type>Safir.Dob.QueueRule</type>
          <arrayElements>
            <arrayElement>
              <!-- The first array item is the default queue lengths.
                   We match all connections. -->
              <object>
                <name>Safir.Dob.QueueRule</name>
                <members>
                  <member>
                    <name>MessageInQueueCapacity</name>
                    <value>10</value>
                  </member>
                  <member>
                    <name>MessageOutQueueCapacity</name>
                    <value>10</value>
                  </member>
                  <member>
                    <name>RequestInQueueCapacity</name>
                    <value>10</value>
                  </member>
                  <member>
                    <name>RequestOutQueueCapacity</name>
                    <value>10</value>
                  </member>
                </members>
              </object>
            </arrayElement>
            <arrayElement>
              <!--  long_message_in_queues -->
              <object>
                <name>Safir.Dob.QueueRule</name>
                <members>
                  <member>
                    <name>ConnectionNameRegex</name>
                    <value>.*_me..age_in_</value>
                  </member>
                  <member>
                    <name>MessageInQueueCapacity</name>
                    <value>20</value>
                  </member>
                  <member>
                    <name>MessageOutQueueCapacity</name>
                    <value>10</value>
                  </member>
                  <member>
                    <name>RequestInQueueCapacity</name>
                    <value>10</value>
                  </member>
                  <member>
                    <name>RequestOutQueueCapacity</name>
                    <value>10</value>
                  </member>
                </members>
              </object>
            </arrayElement>
            <arrayElement>
              <!--  long_message_out_queues -->
              <object>
                <name>Safir.Dob.QueueRule</name>
                <members>
                  <member>
                    <name>ConnectionNameRegex</name>
                    <value>ssage_out</value>
                  </member>
                  <member>
                    <name>MessageInQueueCapacity</name>
                    <value>10</value>
                  </member>
                  <member>
                    <name>MessageOutQueueCapacity</name>
                    <value>20</value>
                  </member>
                  <member>
                    <name>RequestInQueueCapacity</name>
                    <value>10</value>
                  </member>
                  <member>
                    <name>RequestOutQueueCapacity</name>
                    <value>10</value>
                  </member>
                </members>
              </object>
            </arrayElement>
            <arrayElement>
              <!--  long_request_in_queues -->
              <object>
                <name>Safir.Dob.QueueRule</name>
                <members>
                  <member>
                    <name>ConnectionNameRegex</name>
                    <value>long_request_in_queues</value>
                  </member>
                  <member>
                    <name>MessageInQueueCapacity</name>
                    <value>10</value>
                  </member>
                  <member>
                    <name>MessageOutQueueCapacity</name>
                    <value>10</value>
                  </member>
                  <member>
                    <name>RequestInQueueCapacity</name>
                    <value>20</value>
                  </member>
                  <member>
                    <name>RequestOutQueueCapacity</name>
                    <value>10</value>
                  </member>
                </members>
              </object>
            </arrayElement>
            <arrayElement>
              <!--  long_request_out_queues -->
              <object>
                <name>Safir.Dob.QueueRule</name>
                <members>
                  <member>
                    <name>ConnectionNameRegex</name>
                    <value>long_request_o.._queues</value>
                  </member>
                  <member>
                    <name>MessageInQueueCapacity</name>
                    <value>10</value>
                  </member>
                  <member>
                    <name>MessageOutQueueCapacity</name>
                    <value>10</value>
                  </member>
                  <member>
                    <name>RequestInQueueCapacity</name>
                    <value>10</value>
                  </member>
                  <member>
                    <name>RequestOutQueueCapacity</name>
                    <value>20</value>
                  </member>
                </members>
              </object>
            </arrayElement>
          </arrayElements>
        </parameter>
    </parameters>
</class>
