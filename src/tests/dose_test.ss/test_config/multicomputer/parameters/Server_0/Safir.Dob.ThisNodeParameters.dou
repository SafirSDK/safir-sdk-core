<?xml version="1.0" encoding="utf-8" ?>
<class xmlns="urn:safir-dots-unit" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <summary>Defines the node number of this node. This file must be different in all nodes.</summary>
    <name>Safir.Dob.ThisNodeParameters</name>
    <baseClass>Safir.Dob.Parametrization</baseClass>
    <parameters>
      <!-- The weird patterns below are to match the proper addresses on two different test networks:
           192.168.x.y and 10.68.x.y. This is just how our test systems are set up. -->
      
        <parameter>
            <summary>Address and port of the control channel</summary>
            <name>ControlAddress</name>
            <type>String</type>
            <value>1*.*68.*.*:30000</value>
        </parameter>

        <parameter>
            <summary>Address and port of the data channel</summary>
            <name>DataAddress</name>
            <type>String</type>
            <value>1*.*68.*.*:40000</value>
        </parameter>

        <parameter>
            <summary>Address and port of seed nodes control channel</summary>
            <name>Seeds</name>
            <type>String</type>
            <array>
              <value>core-multicomputer-test-server-1-slave:30000</value>
            </array>
        </parameter>

        <parameter>
            <summary>A nice name for this node</summary>
            <name>Name</name>
            <type>String</type>
            <value>Server_0</value>
        </parameter>

        <parameter>
            <summary>Node type for this node</summary>
            <name>NodeType</name>
            <type>String</type>
            <value>Server</value>
        </parameter>

    </parameters>
</class>
